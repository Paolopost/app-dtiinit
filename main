#!/bin/bash
#PBS -l nodes=1:ppn=8,walltime=6:00:00
#PBS -N dtiinit

#for karst/carbonate
module load singularity 2> /dev/null

export FSLOUTPUTTYPE=NIFTI_GZ
singularity exec docker://brainlife/mcr:neurodebian1604-r2017a ./compiled/dtiinit

#check output file
if [ -f dt6.json ];
then
    echo "seems to have finished successfully"
    exit 0
else
    echo "failed to produce dt6.json"
    exit 1
fi


